[REGION:graph]
    [OBJECT:BlueprintNode]
        [PARAMLITERAL:type]
            function
        [/PARAMLITERAL]
        [PARAMLITERAL:title]
            Record Event
        [/PARAMLITERAL]
        [PARAM:inputs]
            [OBJECT:BlueprintPin]
                [PARAMLITERAL:type]
                    exec
                [/PARAMLITERAL]
                [PARAMLITERAL:id]
                    In
                [/PARAMLITERAL]
                [PARAMLITERAL:value]
                    
                [/PARAMLITERAL]
            [/OBJECT]
			[OBJECT:BlueprintPin]
                [PARAMLITERAL:type]
                    string
                [/PARAMLITERAL]
                [PARAMLITERAL:id]
                    Return_Value
                [/PARAMLITERAL]
                [PARAMLITERAL:title]
                    Event Name
                [/PARAMLITERAL]
				[PARAMLITERAL:value]
                    WeaponStore.Opened
                [/PARAMLITERAL]
            [/OBJECT]
        [/PARAM]
        [PARAM:outputs]
            [OBJECT:BlueprintPin]
                [PARAMLITERAL:type]
                    exec
                [/PARAMLITERAL]
                [PARAMLITERAL:id]
                    Out
                [/PARAMLITERAL]
                [PARAMLITERAL:value]
                    
